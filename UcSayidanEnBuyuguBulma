#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {

	int a,b,c;
	printf("3 tane sayi giriniz");
	scanf("%d %d %d",&a,&b,&c);
 	if(a>=b && a>=c) 
	 	printf("a en buyuk sayi");
	if (b>=a && b>=c)
	 	printf("b en buyuk sayi");
	if (c>=a && c>=b)
		printf("c en buyuk sayi");
	return 0;
}

//if ifadesinin altında tek satır varsa {} kullanmamıza gerek yok
