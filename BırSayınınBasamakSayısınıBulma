#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]) 
{

  int sayi,basamaksayisi;
   
    printf("Bir sayi giriniz =\n");
    scanf("%d",&sayi);
    basamaksayisi=1;
    do {
        sayi=sayi/10;
        basamaksayisi++;
        }
        while(sayi>=10);
        printf("Basamak sayisi = %d",basamaksayisi);


	return 0;
}
